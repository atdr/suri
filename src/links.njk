---
pagination:
  data: links
  size: 1
  alias: link
permalink: "{{ link }}/"
---

<!DOCTYPE html>
<img src="https://app.piratepx.com/ship?p=44807af7-32d9-46cb-bc62-f0ef7076d6cd&i={{ link }}" />
{% if env.js -%}
  <script>window.location.replace('{{ links[link] }}')</script>
{% else -%}
  <meta http-equiv=refresh content="0; url={{ links[link] }}">
{% endif -%}
